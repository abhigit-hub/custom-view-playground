<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomView_01">
        <attr name="square_color" format="color" />
    </declare-styleable>

    <declare-styleable name="CustomView_02_05">
        <attr name="faceColor" format="color" />
        <attr name="eyesColor" format="color" />
        <attr name="mouthColor" format="color" />
        <attr name="borderColor" format="color" />
        <attr name="borderWidth" format="dimension" />
        <attr name="sizeMultiplier" format="float" />
        <attr name="state" format="enum">
            <enum name="happy" value="0" />
            <enum name="sad" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomView_04">
        <attr name="colorBar" format="color|reference" />
        <attr name="contentBar" format="color|reference" />
        <attr name="barTitle" format="string" />
        <attr name="message" format="string" />
        <attr name="showMessage" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CustomView_07">
        <attr name="fanOnColor" format="color|reference" />
        <attr name="fanOffColor" format="color|reference" />
        <attr name="selectionIndicators" format="integer" />
    </declare-styleable>
</resources>